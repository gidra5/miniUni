{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "noErrorTruncation": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "noImplicitAny": false,
    "types": [
      "vitest/importMeta"
    ]
  },
  "include": [
    "./src"
  ]
}